<h1>Landmarks</h1>

<%Landmark.all.each do |landmark|%>
  <a href="/landmarks/<%=landmark.id%>"><%=landmark.name%></a><br>
  <label>Year Completed: <%=landmark.year_completed%></label>
  <br><br>
<% end %>
<br>

<button type="button" name="button"><h2><a href="/landmarks/new">New Landmark</a></h2></button>
